{
  "compilerOptions": {
    "target": "es2022",
    "lib": [
      "esnext"
    ],
    "module": "commonjs",
    "moduleResolution": "bundler",
    "baseUrl": "./",
    "paths": {},
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "./scripts"
  ]
}
